---
import TechLine from "../../components/TechLine.astro";
import Prose from "../../components/Prose.astro";
import { getCollection } from 'astro:content';
const techs = await getCollection('tech');
import TechLayout from "../../layouts/TechLayout.astro";
import '@styles/custom.css';
---

<TechLayout title="Tech">

  <div class="w-full bg-purple-700 rounded-t-lg">
    <h3 class="w-fit text-3xl font-bold relative my-2 px-4 py-3 text-white">Tech Section</h3>
  </div>
    <ul>
      {techs.map(tech => (
        <TechLine tech={tech} />
      ))}
    </ul>

</div>
<div class="w-1/3">
  
</div>



</TechLayout>