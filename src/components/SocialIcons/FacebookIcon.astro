---
interface Props {
  class?: string;
  href?: string;
  fill?: string;
}
const { class: className = 'w-10 h-10', href = '#', fill = 'currentColor' } = Astro.props;
---


<a href={href} class="text-gray-500 hover:text-gray-900 dark:hover:text-white dark:text-gray-400">
    <svg class={className} aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill={fill} viewBox="0 0 8 19">
        <path fill-rule="evenodd" d="M6.135 3H8V0H6.135a4.147 4.147 0 0 0-4.142 4.142V6H0v3h2v9.938h3V9h2.021l.592-3H5V3.591A.6.6 0 0 1 5.592 3h.543Z" clip-rule="evenodd"/>
  </svg>
</a>