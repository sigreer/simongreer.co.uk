{
  "name": "@sigreer/simongreercouk",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler types && astro dev",
    "start": "wrangler types && astro dev",
    "build": "astro check && astro sync --clear && astro build --clear",
    "buildw": "wrangler types && astro sync --clear && astro build --clear",
    "preview": "astro build && wrangler pages dev",
    "astro": "astro",
    "deploy": "wrangler types && astro build && wrangler pages deploy",
    "dump": "wrangler types && wrangler deploy --dry-run",
    "sync": "astro sync --clear && wrangler types",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "clean": "rm -rf ./node_modules && rm bun.lockb && rm -rf ./.astro && rm -rf ./.wrangler && bun pm cache rm && rm -rf ./dist",
    "regen": "bun install && astro sync --clear && wrangler types"
  },
